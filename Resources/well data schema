-- creating tables for well data
Create Table well_data (
	well_id VARCHAR NOT Null,
	Lease_name VARCHAR(50) NOT NULL,
	well_number VARCHAR (25),
	operator_company VARCHAR(50),
	county VARCHAR(50),
	Landing_zone VARCHAR(50),
	Surf_lat DECIMAL,
	Surf_long DECIMAL,
	BH_lat DECIMAL,
	BH_long DECIMAL,
	Date_spud DATE,
	Lateral_len BIGINT,
	Date_completed DATE,
	Total_fluid BIGINT,
	Total_proppant BIGINT,
	Well_spacing DECIMAL,
	Well_name VARCHAR,
	fluid BIGINT,
	prop BIGINT,
	avg_ppg DECIMAL,
	oil_eur BIGINT,
	gas_eur BIGINT,
	PRIMARY KEY (well_id),
	UNIQUE (well_id)
	
);

SELECT * FROM well_data;
